
pipeline {

 agent any
   stages {
       stage("git")
       {
	     steps
	         {
                 git credentialsId: 'github', url: 'https://github.com/subhpanda/myweb.git'
		 }
	} 
	stage("maven")
        {
	      steps
	       {
                   sh 'mvn clean package'
               }
	} 
	stage("echo  test")
        {
	      steps
	       {
                   echo "subham is  good boy"
               }
	}
   
   }

}
