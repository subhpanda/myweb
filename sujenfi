
pipeline {

 agent any
   stages {
       stage("git")
       {
	     steps
	         {
                 git credentialsId: 'github', url: 'https://github.com/subhpanda/myweb.git'
		 }
	} 
	stage("maven")
        {
	      steps
	       {
                   sh 'mvn clean package'
		   sh "mv target/*.war  target/myweb.war "
               }
	} 
	
	
	stage("maven")
        {
	      steps
	       {
                   echo "subham is a good a boy"
               }
	} 
	

   
   }

}
